{
  "name": "stuhouses",
  "version": "1.0.0",
  "description": "Student housing platform with multilingual support",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start:frontend": "cd frontend && npm run start",
    "start:backend": "cd backend && npm run start",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "concurrently \"npm run build:frontend\" \"npm run build:backend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm install",
    "install:all": "npm install && concurrently \"npm run install:frontend\" \"npm run install:backend\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "clean": "rm -rf node_modules && rm -rf frontend/node_modules && rm -rf backend/node_modules"
  },
  "keywords": [
    "student",
    "accommodation",
    "housing",
    "multilingual",
    "next.js",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 